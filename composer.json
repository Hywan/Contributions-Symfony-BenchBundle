{
    "name": "hoathis/bench-bundle",
    "description": "The Hoa\\Bench Symfony 2 bundle.",
    "type"       : "symfony-bundle",
    "keywords"   : ["library", "bundle", "symfony", "bench"],
    "homepage"   : "http://hoa-project.net/",
    "license"    : "BSD-3-Clause",
    "authors"    : [
        {
            "name": "Julien Bianchi",
            "email": "julien.bianchi@hoa-project.net"
        },
        {
            "name" : "Ivan Enderlin",
            "email": "ivan.enderlin@hoa-project.net"
        },
        {
            "name"    : "Hoa community",
            "homepage": "http://hoa-project.net/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "hoa/bench": "*",
        "symfony/http-kernel": "~2.2",
        "symfony/console": "~2.2",
        "symfony/config": "~2.2",
        "symfony/dependency-injection": "~2.2",
        "twig/twig": "~1.11"
    },
    "require-dev": {
        "atoum/atoum": "dev-master"
    },
    "target-dir": "Hoathis/Bundle/BenchBundle",
    "autoload": {
        "psr-0": {
            "Hoathis\\Bundle\\BenchBundle": "."
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
